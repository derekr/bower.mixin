{{#each packages}}
<li data-package-name="{{lower this.name}}" class="visible">
  <div class="package-info">
    <span class="name">{{this.name}}</span>
    <span class="url">{{this.url}}</span>
  </div>
  <span class="action">
    <button data-package-name="{{this.name}}">Install</button>
  </span>
</li>
{{/each}}